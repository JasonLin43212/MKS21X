public class Barcode{
  private String zip;

  public Barcode(String zip){
    this.zip = zip;
  }

  public String getZip(){
    return zip;
  }

  public int getCheckSum(String zip){
    return Integer.parseInt(zip) % 10;
  }
  
  public String getCode(){
    zip = zip + getCheckSum();
  }

  public String toString(){
    
  }
}